# Browser downloads
downloads/
screenshots/

# Playwright
test-results/
playwright-report/
playwright/.cache/

# OS
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/
*.swp
*.swo

# Data files
data/
port_list.txt
test-*
